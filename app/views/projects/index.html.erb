<main role="main">
<p id="notice"><%= notice %></p>
  
<div class="jumbotron">
<div class="container">
  <h1 class="display-3">Past Work</h1>
  <p>This website houses information technology projects.</p>
  <p><a class="btn btn-primary btn-lg" href="#" role="button">About the author &raquo;</a></p>
</div>
</div>


<div class="container">
    <div class="row">
      
  
        <% @projects.each do |project| %>
          <div class="col-xl-4">
            <h2><%= project.title %></h2>
            <p><%= project.description %></p>
            <p><%= link_to 'Show', project %></p>
            <p><%= link_to 'Edit', edit_project_path(project) %></p>
            <p><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></p>
          </div>
        <% end %>
      
    </div>

    <hr>

  </div> <!-- /container -->
      
    
  



<br>

<%= link_to 'New Project', new_project_path %>


</main>